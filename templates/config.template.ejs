exports.default = {
    <% props.modules.forEach(module => { %>
        <%= module.name %>: {
            enabled: true,
            config: <%- include(`${module.name}-config.template.ejs`, module.defaultConfig) %>,
        },
    <% }); %>
    reporting: {
        enabled: <%= props.reporting.enabled %>,
        config: <%- include(`reporting-config.template.ejs`, props.reporting.defaultConfig) %>,
    }
};